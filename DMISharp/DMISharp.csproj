<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Authors>Bobbahbrown</Authors>
    <Company>MelonMesa</Company>
    <Description>Library for handling BYOND DMI files.</Description>
    <Copyright>Copyright (c) 2019 MelonMesa</Copyright>
    <PackageProjectUrl>https://melonmesa.com</PackageProjectUrl>
    <RepositoryUrl>github.com/bobbahbrown/DMISharp</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <AssemblyVersion>1.1.2.0</AssemblyVersion>
    <Version>1.1.2</Version>
    <PackageIconUrl>https://melonmesa.com/storage/dmisharp.png</PackageIconUrl>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageLicenseExpression>GPL-3.0-only</PackageLicenseExpression>
    <PackageTags>DMI BYOND SpaceStation13</PackageTags>
    <PackageReleaseNotes>Patched bug with DMIMetadata instantiation when a DMI header contained '#START DMI' instead of '# START DMI'.</PackageReleaseNotes>
    <FileVersion>1.1.2.0</FileVersion>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="MetadataExtractor" Version="2.1.0" />
    <PackageReference Include="SixLabors.ImageSharp" Version="1.0.0-beta0006" />
  </ItemGroup>

</Project>
